{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"lint:fix": "eslint --fix \"src/**/*.{js,ts,svelte}\" && prettier --write .",
		"format": "prettier --write .",
		"generate:types": "tsx ./src/lib/directus/generateDirectusTypes.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.35.0",
		"@lucide/svelte": "^0.482.0",
		"@sveltejs/enhanced-img": "^0.6.1",
		"@sveltejs/kit": "^2.37.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.4",
		"@types/node": "^22.18.1",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.8.0",
		"clsx": "^2.1.1",
		"directus-sdk-typegen": "^0.2.1",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.1",
		"formsnap": "^2.0.1",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.42.0",
		"vite": "^7.1.4"
	},
	"dependencies": {
		"@directus/sdk": "^19.1.0",
		"@directus/visual-editing": "^1.1.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"class-variance-authority": "^0.7.1",
		"dotenv": "^17.2.2",
		"lodash-es": "^4.17.21",
		"mode-watcher": "^1.1.0",
		"p-queue": "^8.1.0",
		"zod": "^3.25.76"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"packageManager": "pnpm@10.13.1"
}
